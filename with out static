package java_project;
class salary1
{
	public int id;
	public void Read_salary()
	{
		id=25;
		
	}
	public void display_salary()
	{
		System.out.println(id);
	}
}
public class salary {
	

	public static void main(String[] args) {
		salary1 k=new salary1();
		k.Read_salary();
		k.display_salary();
		

	}

}
