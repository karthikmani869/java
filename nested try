package sunny;

public class multicatch {

	public static void main(String[] args) 
	{
		String s=null;
		try
		{
			System.out.println(s.length());
		}
		catch(NullPointerException e)
		{
			System.out.println(e);
		}
		catch(Exception n)
		{
			System.out.println(n);
		}
		finally
		{
			System.out.println("hello ");
		}
		
		
	}

}
