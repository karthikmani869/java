package java_project;
import java.util.*;
class Example{
	public void Bunny(int a,int b,int c)
	{
		c=a+b;
		System.out.println(c);
		
	}
	public void Bunny(int a,int b) 
	{
		if(a>b)
			System.out.println("a is big");
		else {
			System.out.println("b is big");
		}
	}
	public void Bunny(int a)
	{
		System.out.println("hello bunny");
	}
}

public class over_load {

	public static void main(String args[]) {
		Scanner s=new Scanner(System.in);
		Example z=new Example();
		int a,b,c;
		System.out.println("enter a,b,c values");
		a=b=c=s.nextInt();
		
		z.Bunny(a);
		z.Bunny(a, b);
		z.Bunny(a, b, c);
}
}
