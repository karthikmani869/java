package bunny;
import java.util.*;
public class arrays {
	public static void main(String[] args) {
	int a[]=new int [10];
	int n,i;

	Scanner s =new Scanner(System.in);
	System.out.println("enter n  values");
	
	n=s.nextInt();
	System.out.println("enter array  values");
	
	int sum=0;
	
	
	for(i=0;i<n;i++)
	{
		a[i]=s.nextInt();
	}
	for(i=0;i<n;i++)
	{
	sum=sum+a[i];
	}
	System.out.println(sum);
	}
}
